    }
  },
  "typography": {
    "fontFamily": {
      "display": ["Space Grotesk", "system-ui", "sans-serif"],
      "body": ["Inter", "system-ui", "sans-serif"]
    },
    "fontSize": {
      "display-xl": {
        "size": "4.5rem",
        "lineHeight": "1.1",
        "letterSpacing": "-0.02em",
        "fontWeight": "700"
      },
      "display-lg": {
        "size": "3.75rem",
        "lineHeight": "1.1",
        "letterSpacing": "-0.02em",
        "fontWeight": "700"
      },
      "display-md": {
        "size": "3rem",
        "lineHeight": "1.15",
        "letterSpacing": "-0.01em",
        "fontWeight": "700"
      },
      "display-sm": {
        "size": "2.25rem",
        "lineHeight": "1.2",
        "letterSpacing": "-0.01em",
        "fontWeight": "600"
      },
      "display-xs": {
        "size": "1.875rem",
        "lineHeight": "1.3",
        "letterSpacing": "0",
        "fontWeight": "600"
      }
    },
    "lineHeight": {
      "relaxed": "1.6"
    }
  },
  "boxShadow": {
    "soft-sm": "0 2px 8px rgba(180, 167, 214, 0.12)",
    "soft-md": "0 4px 16px rgba(180, 167, 214, 0.18)",
    "soft-lg": "0 8px 24px rgba(180, 167, 214, 0.22)",
    "glow-purple": "0 0 30px rgba(180, 167, 214, 0.5)",
    "glow-pink": "0 0 30px rgba(255, 179, 217, 0.5)",
    "glow-cyan": "0 0 30px rgba(168, 230, 207, 0.5)",
    "glass": "0 8px 32px 0 rgba(180, 167, 214, 0.15)",
    "neon": "0 0 5px rgba(180, 167, 214, 0.6), 0 0 20px rgba(180, 167, 214, 0.4)"
  },
  "backgroundImage": {
    "gradient-mesh": "linear-gradient(135deg, #B4A7D6 0%, #FFB3D9 50%, #A8E6CF 100%)",
    "gradient-soft": "linear-gradient(135deg, #FAF9FC 0%, #FFF5FB 50%, #F0FCF7 100%)",
    "gradient-radial": "radial-gradient(circle at 50% 0%, rgba(180, 167, 214, 0.15) 0%, rgba(255, 179, 217, 0.08) 50%, transparent 80%)"
  },
  "spacing": {
    "18": "4.5rem",
    "88": "22rem",
    "100": "25rem",
    "112": "28rem",
    "128": "32rem"
  },
  "borderRadius": {
    "default": "0.5rem"
  },
  "backdropBlur": {
    "xs": "2px"
  },
  "animations": {
    "fade-up": {
      "duration": "0.6s",
      "timing": "ease-out",
      "keyframes": {
        "0%": { "opacity": "0", "transform": "translateY(20px)" },
        "100%": { "opacity": "1", "transform": "translateY(0)" }
      }
    },
    "fade-in": {
      "duration": "0.4s",
      "timing": "ease-out",
      "keyframes": {
        "0%": { "opacity": "0" },
        "100%": { "opacity": "1" }
      }
    },
    "slide-in": {
      "duration": "0.4s",
      "timing": "ease-out",
      "keyframes": {
        "0%": { "transform": "translateX(-100%)" },
        "100%": { "transform": "translateX(0)" }
      }
    },
    "float": {
      "duration": "6s",
      "timing": "ease-in-out",
      "iteration": "infinite",
      "keyframes": {
        "0%, 100%": { "transform": "translateY(0)" },
        "50%": { "transform": "translateY(-20px)" }
      }
    },
    "bounce-slow": {
      "duration": "3s",
      "timing": "ease-in-out",
      "iteration": "infinite"
    },
    "wiggle": {
      "duration": "1s",
      "timing": "ease-in-out",
      "iteration": "infinite",
      "keyframes": {
        "0%, 100%": { "transform": "rotate(-3deg)" },
        "50%": { "transform": "rotate(3deg)" }
      }
    },
    "pulse-slow": {
      "duration": "3s",
      "timing": "cubic-bezier(0.4, 0, 0.6, 1)",
      "iteration": "infinite"
    },
    "spin-slow": {
      "duration": "8s",
      "timing": "linear",
      "iteration": "infinite"
    },
    "ping-slow": {
      "duration": "2s",
      "timing": "cubic-bezier(0, 0, 0.2, 1)",
      "iteration": "infinite"
    },
    "gradient": {
      "duration": "8s",
      "timing": "ease",
      "iteration": "infinite",
      "keyframes": {
        "0%, 100%": { "backgroundPosition": "0% 50%" },
        "50%": { "backgroundPosition": "100% 50%" }
      }
    },
    "blob": {
      "duration": "7s",
      "iteration": "infinite",
      "keyframes": {
        "0%": { "transform": "translate(0px, 0px) scale(1)" },
        "33%": { "transform": "translate(30px, -50px) scale(1.1)" },
        "66%": { "transform": "translate(-20px, 20px) scale(0.9)" },
        "100%": { "transform": "translate(0px, 0px) scale(1)" }
      }
    },
    "tilt": {
      "duration": "10s",
      "timing": "linear",
      "iteration": "infinite",
      "keyframes": {
        "0%, 50%, 100%": { "transform": "rotate(0deg)" },
        "25%": { "transform": "rotate(1deg)" },
        "75%": { "transform": "rotate(-1deg)" }
      }
    }
  }
}
